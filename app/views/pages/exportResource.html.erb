         <% @data["data"].each do |tr| %>              
              <div class="transaction-item">
              <span><%= tr["Titre"]  %></span>
              <span><%= tr["Description"]%></span>
              <span><%= tr["Date de création"] %></span>
              <span ><%= tr["Tags"] %></span>
              <span><%= tr["Type"] %></span>
              <span><%= tr["URL"] %></span>
              </div>
            <% end %>
        </div>
      </div>
    </div>
</body>
</html>
